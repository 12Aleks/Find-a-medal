generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id        String   @id @default(uuid())
  nickName  String?
  firstName String
  lastName  String
  email     String
  phone     String?
  avatarUrl String?
  interests String?
  skype     String?
  facebook  String?
  x         String?
  youtube   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
